{
  "name": "@tg-media-vault/api",
  "version": "0.0.1",
  "description": "Backend App saves & provides access to the downloaded media content",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:update-snapshots": "jest -u",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.ts",
    "db:reset": "npx prisma migrate reset"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "10.2.1",
    "@nestjs/core": "10.2.1",
    "@nestjs/microservices": "10.2.1",
    "@nestjs/platform-express": "^10.4.15",
    "@prisma/client": "5.11.0",
    "axios": "^1.8.4",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "mime-types": "^3.0.1",
    "node-telegram-bot-api": "^0.66.0",
    "p-queue": "^6.6.2",
    "prisma": "5.5.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.16",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.79",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.4",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20.5.6",
    "@types/node-telegram-bot-api": "^0.64.10",
    "jest": "29.6.4",
    "source-map-support": "^0.5.21",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "form-data": "^4.0.0"
  },
  "prisma": {
    "schema": "db/schema.prisma",
    "migrateOutput": "db/migrations/"
  }
}
